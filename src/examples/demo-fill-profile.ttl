         ff:hasValue "@prefix sh: <http://www.w3.org/ns/shacl#> .\n@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .\n@prefix ff: <https://foerderfunke.org/default#> .\n@prefix foaf: <http://xmlns.com/foaf/0.1/> .\n@prefix fim: <https://neu.fimportal.de/fields/baukasten/> .\n@prefix wd: <http://www.wikidata.org/entity/> .\n\n# DATAFIELDS\n\nff:hasChild a ff:DataField ;\n    ff:shaclShape [\n    \ta sh:PropertyShape ;\n        sh:path ff:hasChild ;\n        sh:name \"has child\" ;\n        sh:nodeKind sh:IRI ;\n        sh:description \"add child\" ;\n        ff:pointsToInstancesOf ff:Child ;\n\t] .\n\nfoaf:age a ff:DataField ;\n    ff:shaclShape [\n        a sh:PropertyShape ;\n        sh:path foaf:age ;\n        ff:wikidataId wd:Q185836 ;\n        sh:name \"Age\" ;\n        sh:datatype xsd:integer ;\n        sh:minInclusive 0 ;\n\t\tsh:maxCount 1 ;\n\t] .\n\nff:citizenship a ff:DataField ;\n\tff:shaclShape [\n        a sh:PropertyShape ;\n        sh:path ff:citizenship ;\n        sh:name \"Citizenship\" ;\n        sh:in (ff:cit-ao-ger ff:cit-ao-eu ff:cit-ao-3rd) ;\n\t] .\n\nff:cit-ao-ger a ff:AnswerOption ; ff:title \"German\" .\nff:cit-ao-eu a ff:AnswerOption ; ff:title \"EU\" .\nff:cit-ao-3rd a ff:AnswerOption ; ff:title \"Third country (non-EU)\" .\n\nff:hasResidence a ff:DataField ;\n    ff:shaclShape [\n        a sh:PropertyShape ;\n        sh:path ff:hasResidence ;\n        sh:name \"Residence\" ;\n       \tff:hasFimId fim:F03010085 ;\n        ff:hasFimVersion \"1.0\" ;\n        sh:datatype xsd:string ;\n    ] .\n\nff:residesInState a ff:DataField ;\n    ff:shaclShape [\n        a sh:PropertyShape ;\n        sh:path ff:residesInState ;\n        sh:name \"Federal state\" ;\n        sh:datatype xsd:string ;\n    ] .\n\n# CLASSES\n\nff:Citizen a ff:Class .\nff:Child a ff:Class .\n\n# INFERENCE RULES\n\nff:DeriveFederalStateFromCity a ff:MaterializationRule ;\n\tff:sparqlConstructQuery \"\"\"\n    \tPREFIX ff: <https://foerderfunke.org/default#>\n        CONSTRUCT {\n            ?person ff:residesInState ?state .\n        } WHERE {\n            ?person ff:hasResidence ?city .\n            ff:CityStateMap ff:mappedCityToState ?mapping .\n            ?mapping ff:city ?city ;\n                ff:state ?state .\n        }\n\t\"\"\" .\n\n# CONSTANTS\n\nff:CityStateMap a ff:MapOfConstants ;\n    ff:mappedCityToState [\n        ff:city \"Bad Belzig\" ;\n        ff:state \"Brandenburg\"\n    ] ;\n    ff:mappedCityToState [\n        ff:city \"Leipzig\" ;\n        ff:state \"Saxony\"\n    ] ;\n    ff:mappedCityToState [\n        ff:city \"Augsburg\" ;\n        ff:state \"Bavaria\"\n    ] .".
ff:node3 a ff:Node;
